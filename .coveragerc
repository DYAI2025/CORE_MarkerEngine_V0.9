[run]
source = .
omit =
    */tests/*
    */test_*
    setup.py
    api_service.py
    validate_system.py
    unify_activation.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
